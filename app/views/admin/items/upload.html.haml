%h2 Upload CSV

= form_tag import_csv_admin_items_path, multipart: true do
  .form-inputs
    = file_field_tag :csv_file
  .form-actions
    = submit_tag "Upload", class: 'btn btn-primary'

%hr

%h2 Upload Images

= form_tag import_images_admin_items_path, multipart: true do
  .form-inputs
    = file_field_tag 'images[]', multiple: true
  .form-actions
    = submit_tag "Upload", class: 'btn-primary btn'
